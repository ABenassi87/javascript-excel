{
  "name": "javascript-excel",
  "author": "Afzal Ahmad (securedeveloper@gmail.com)",
  "description": "A lightweight library to build excel sheet with JavaScript",
  "keywords": [
    "js-excel",
    "javascript-excel",
    "excel",
    "xlsx",
    "js-csv",
    "javascript-csv",
    "csv"
  ],
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "bundle": "webpack",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "test": "mocha test/**/*.js"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-typescript": "7.0.0",
    "@types/file-saver": "1.3.0",
    "@types/jszip": "3.1.4",
    "babel-loader": "8.0.0",
    "mocha": "5.2.0",
    "typescript": "3.0.3",
    "webpack": "4.18.0"
  },
  "dependencies": {
    "file-saver": "1.3.8",
    "jszip": "3.1.5"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/securedeveloper/javascript-excel.git"
  },
  "bugs": {
    "url": "https://github.com/securedeveloper/javascript-excel/issues"
  },
  "homepage": "https://github.com/securedeveloper/javascript-excel#readme"
}
